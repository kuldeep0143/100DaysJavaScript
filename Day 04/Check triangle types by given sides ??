
const checkTriangle = (a,b,c) => {
    if( a===b && b===c) {
        return "equilateral"
    }
    else if (a===b || b === c || a === c){
        return "isosceles"
    }
     else if (a != b || b != c) {
         return "scalene"
     }

}

console.log(checkTriangle(45,35,55)) //scalene
console.log(checkTriangle(33,37,35)) //isosceles
console.log(checkTriangle(45,45,45)) //equilateral
